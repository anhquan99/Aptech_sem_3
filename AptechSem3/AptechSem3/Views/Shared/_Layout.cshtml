                <div class="preloader-circle"></div>
                <div class="preloader-img pere-text">
                    <img src="~/Content/CandidateWeb/assets/img/logo/loder.jpg" alt="">
                </div>
            </div>
        </div>
    </div>
    <!-- Preloader Start -->
    <header>
        <!-- Header Start -->
        <div class="header-area">
            <div class="main-header ">
                <div class="header-bottom  header-sticky">
                    <div class="container">
                        <div class="row align-items-center">
                            <!-- Logo -->
                            <div class="col-xl-2 col-lg-2">
                                <div class="logo">
                                    <a href="#"><img src="~/Content/CandidateWeb/assets/img/logo/logo.png" alt=""></a>
                                </div>
                            </div>
                            <div class="col-xl-10 col-lg-10">
                                <div class="menu-wrapper  d-flex align-items-center justify-content-end">
                                    <!-- Main-menu -->
                                    <div class="main-menu d-none d-lg-block">
                                        <nav>
                                            <ul id="navigation">
                                                <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                                                <li><a href="@Url.Action("JobPost", "Home")">Jobs</a></li>
                                                @if (Session["username"] == null)
                                                {
                                                    <li><a href="@Url.Action("Login","Home")">Login</a></li>
                                                }
                                                else
                                                {
                                                    <li><a href="@Url.Action("Logout","Home")">Logout</a></li>
                                                }
                                            </ul>
                                        </nav>
                                    </div>
                                </div>
                            </div>
                            <!-- Mobile Menu -->
                            <div class="col-12">
                                <div class="mobile_menu d-block d-lg-none"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Header End -->
    </header>

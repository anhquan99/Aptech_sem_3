    <link rel="manifest" href="site.webmanifest">
    <link rel="shortcut icon" type="image/x-icon" href="CandidateWeb/assets/img/favicon.ico">

    <!-- CSS here -->
    @Styles.Render("~/Content/CandidateWeb/assets/css/bootstrap.min.css")

<!DOCTYPE html>
<html style="height:auto;">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Manager | @ViewBag.Title</title>
    <script src="https://kit.fontawesome.com/e69e3b6c2e.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.5.0/Chart.min.js"></script>
    <link rel="stylesheet" href="@Url.Content("~/Content/css/daterangepicker.css")">
    <script async="" src="//www.google-analytics.com/analytics.js"></script>

    <link rel="stylesheet" href="@Url.Content("~/Content/css/adminlte.min.css")">
    <link href="https://fonts.googleapis.com/css2?family=Source+Sans+Pro:ital,wght@0,200;0,300;0,400;0,600;0,700;0,900;1,200;1,300;1,400;1,600;1,700;1,900&display=swap" rel="stylesheet">

</head>
<body class="sidebar_mini" style="height: auto;">
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="@Url.Action("Index","Manager")" class="nav-link">Home</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="#" class="nav-link">Contact</a>
                </li>
            </ul>
        </nav>
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="@Url.Action("Index","Manager")" class="brand-link">
                <img src="@Url.Content("~/Content/img/AdminLTELogo.png")" class="brand-image img-circle elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-light">Webster</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="@Url.Content("~/Content/img/user2-160x160.jpg")" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block">Alexander Pierce</a>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->
                        <li class="nav-header">TEST</li>
                        <li class="nav-item has-treeview menu-open">
                            <a href="#" class="nav-link active">
                                <i class="nav-icon fas fa-pen"></i>
                                <p>
                                    Test
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("CreateTest","Manager")" class="nav-link">
                                        <i class="nav-icon far fa-plus-square"></i>
                                        <p>Create Test</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("ShowTests","Manager")" class="nav-link">
                                        <i class="nav-icon far fa-eye"></i>
                                        <p>View All Tests</p>
                                    </a>
                                </li>

                            </ul>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link active">
                                <i class="nav-icon fas fa-question"></i>
                                <p>
                                    Questions & Answers
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("CreateQA","Manager")" class="nav-link">
                                        <i class="nav-icon far fa-plus-square"></i>
                                        <p>Create Q & A</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("ShowQuestions","Manager")" class="nav-link">
                                        <i class="nav-icon far fa-eye"></i>
                                        <p>View All Q & A</p>
                                    </a>
                                </li>

                            </ul>
                        </li>
                        <li class="nav-header">JOB</li>
                        <li class="nav-item has-treeview menu-open">
                            <a href="#" class="nav-link active">
                                <i class="nav-icon fas fa-suitcase"></i>
                                <p>
                                    Job Post
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("CreatePost","Manager")" class="nav-link">
                                        <i class="nav-icon far fa-plus-square"></i>
                                        <p>Create Job Post</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("ShowPosts","Manager")" class="nav-link">
                                        <i class="nav-icon far fa-eye"></i>
                                        <p>View All Job Post</p>
                                    </a>
                                </li>

                            </ul>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link active">
                                <i class="nav-icon fas fa-paste"></i>
                                <p>
                                    Job Applications
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">

                                <li class="nav-item">
                                    <a href="@Url.Action("ShowApplications","Manager", new { status = 0})" class="nav-link">
                                        <i class="nav-icon far fa-eye"></i>
                                        <p>View Job Applications</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("ShowApplications","Manager", new { status = 1})" class="nav-link">
                                        <i class="nav-icon far fa-eye"></i>
                                        <p>View Approved</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("ShowApplications","Manager" , new { status = -1})" class="nav-link">
                                        <i class="nav-icon far fa-eye"></i>
                                        <p>View Unapproved</p>
                                    </a>
                                </li>

                            </ul>
                        </li>

                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link active">
                                <i class="nav-icon fas fa-users"></i>
                                <p>
                                    Candidates
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("ShowCandidates","Manager")" class="nav-link">
                                        <i class="nav-icon far fa-eye"></i>
                                        <p>View Candidates</p>
                                    </a>
                                </li>


                            </ul>
                        </li>


                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>
        <div class="content-wrapper" style="min-height:1200.88px;">
            <!-- Content Header (Page header) -->
            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1 class="m-0 text-dark">@ViewBag.Title</h1>
                        </div><!-- /.col -->
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
                                <li class="breadcrumb-item active">@ViewBag.Title</li>
                            </ol>
                        </div><!-- /.col -->
                    </div><!-- /.row -->
                </div>
            </div>
            <!-- Main content -->
            @RenderBody()
        </div>
        <footer class="main-footer">
            <strong>Copyright © 2014-2019 <a href="http://adminlte.io">AdminLTE.io</a>.</strong>
            All rights reserved.
            <div class="float-right d-none d-sm-inline-block">
                <b>Version</b> 3.0.4
            </div>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)


</body>
</html>

